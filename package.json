{
  "name": "drips-events-processor",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "commit": "npx cz",
    "prepare": "husky install",
    "format": "prettier --write .",
    "docker:clean": "docker-compose down && docker volume rm events-processor_postgres_data",
    "lint": "prettier --check . && eslint src/**/*.ts",
    "test": "jest --detectOpenHandles --config=jest.config.ts",
    "test:coverage": "jest --coverage",
    "build": "npm run build:contracts && tsc",
    "clean": "rm -rf dist",
    "build:contracts": "./scripts/build-contracts.sh && ts-node ./scripts/codegen-any-chain-types.ts",
    "postbuild": "rm -rf ./dist/src/config/chainConfigs && cp -r ./src/config/chainConfigs ./dist/src/config/chainConfigs",
    "check": "tsc --noEmit",
    "dev": "npx nodemon",
    "start": "node dist/src/index.js",
    "sequelize": "ts-node --transpile-only node_modules/sequelize-cli/lib/sequelize",
    "db:run-migrations": "npm run clean && npm run build && ts-node scripts/run-migrations.ts",
    "db:create-migration": "npm run sequelize migration:generate -- --migrations-path src/db/migrations",
    "dev:db:revert-migration": "npm run sequelize -- db:migrate:undo --migrations-path src/db/migrations --config dist/src/config/sequelizeConfig.js",
    "dev:db:log-pending-migrations": "npm run sequelize -- db:migrate:status --migrations-path src/db/migrations --config dist/src/config/sequelizeConfig.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.0",
    "@types/bull-arena": "^3.0.10",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.5.9",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@efstajas/versioned-parser": "^0.1.4",
    "@types/umzug": "^2.3.9",
    "bee-queue": "^1.5.0",
    "bull-arena": "^4.4.2",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^11.0.6",
    "ethers": "^6.7.1",
    "express": "^4.21.2",
    "get-caller-file": "^2.0.5",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "sequelize": "^6.32.1",
    "umzug": "^3.8.2",
    "winston": "^3.10.0",
    "zod": "^3.24.2"
  }
}
